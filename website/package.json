{
  "name": "sdow",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@tsparticles/react": "^3.0.0",
    "axios": "^1.6.2",
    "classnames": "^2.3.2",
    "connected-react-router": "^6.9.3",
    "d3": "^7.8.5",
    "history": "^5.3.0",
    "lodash": "^4.17.21",
    "prop-types": "^15.8.1",
    "query-string": "^8.1.0",
    "react": "^18.2.0",
    "react-autosuggest": "^10.1.0",
    "react-dom": "^18.2.0",
    "react-dotenv": "^0.1.3",
    "react-helmet": "^6.1.0",
    "react-lazyload": "^3.2.0",
    "react-loadable": "^5.5.0",
    "react-modal": "^3.16.1",
    "react-redux": "^9.0.2",
    "react-router-dom": "^6.20.1",
    "react-scripts": "5.0.1",
    "redux": "^5.0.0",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^3.1.0",
    "styled-components": "^6.1.1",
    "tsparticles": "^3.0.2",
    "typeface-crimson-text": "1.1.13",
    "typeface-quicksand": "1.1.13"
  },
  "devDependencies": {
    "husky": "^8.0.3",
    "lint-staged": "^15.2.0",
    "prettier": "^3.1.0",
    "redux-logger": "^3.0.6",
    "source-map-explorer": "^2.5.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "{src}/**/*.{js,css}": [
      "prettier --write"
    ]
  },
  "scripts": {
    "analyze": "source-map-explorer build/static/js/*",
    "precommit": "lint-staged",
    "start": "react-dotenv && react-scripts start",
    "build": "react-dotenv && react-scripts build",
    "serve": "react-dotenv && serve build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "react-dotenv": {
    "whitelist": ["SDOW_API_URL","WIKIPEDIA_API_URL","WIKIPEDIA_BASE_URL"]
  }
}
